---
interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<svg
  aria-hidden="true"
  xmlns="http://www.w3.org/2000/svg"
  fill="none"
  viewBox="0 0 16 12"
  class={className}
>
  <path
    stroke="currentColor"
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="2"
    d="M1 5.917 5.724 10.5 15 1.5"></path>
</svg>
