---
import Link from "@/components/link.astro";
import SectionContainer from "@/components/section-container.astro";
import { LINKS } from "@/constants/links";

const year = new Date().getFullYear();
---

<SectionContainer class="mt-20" id="contact">
  <footer class="flex justify-center items-center gap-x-5">
    {
      LINKS.map(({ title, url, label }) => (
        <Link url={url} aria-label={label}>
          {title}
        </Link>
      ))
    }
  </footer>
</SectionContainer>
